{
  "name": "animal_facts_and_pictures_generator",
  "version": "1.0.0",
  "description": "A CLI tool to fetch and manage facts about cats and dogs, and to fetch images of cats, dogs, and foxes.",
  "main": "main.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node main.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "cross-env": "^7.0.3"
  },
  "jest": {
    "transform": {},
    "testMatch": ["**/test/**/*.test.mjs"],
    "moduleFileExtensions": ["js", "mjs", "json"],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testEnvironment": "node"
  },
  "keywords": [
    "cat",
    "dog",
    "fox",
    "images",
    "facts",
    "fetcher"
  ],
  "author": "Giulio Davolio Marani",
  "license": "MIT"
}